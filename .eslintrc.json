{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "env": {"browser": true, "es6": true},
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "indent": ["error", 1, {"SwitchCase": 1}],
    "semi": [1, "never"],
    "comma-dangle": [1, "always-multiline"],
    "object-curly-spacing": [2, "never"],
    "key-spacing": 1,
    "jsx-quotes": 1,
    "no-console": ["warn", {"allow": ["info", "warn", "error"]}]
  }
}
